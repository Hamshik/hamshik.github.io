++++++++++++++        run: |
++++++++++++++          echo "Validating GPG signatures..."
++++++++++++++          for file in dist/*; do
++++++++++++++            if [[ "$file" == *.gpg ]]; then
++++++++++++++              gpg --batch --passphrase "${GPG_PASSWORD}" --verify "$file" \
