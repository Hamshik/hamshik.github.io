++++++++++++++          echo "Running security scan on repository..."
++++++++++++++          sudo apt-get update
++++++++++++++          sudo apt-get install -y clamav
++++++++++++++          clamscan -r $WORKING_DIRECTORY > $WORKFLOW_DIR/security-scan.log
++++++++++++++          echo "Security scan completed. Log stored in $WORKFLOW_DIR."
