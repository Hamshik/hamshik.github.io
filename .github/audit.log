++++++++++++++++++++          find $WORKING_DIRECTORY/.github/workflows -type f -name "*.yml" -exec yamllint {} \;
++++++++++++++++++++          echo "YAML validation completed." > $WORKFLOW_DIR/yaml-validation.txt
++++++++++++++++++++
