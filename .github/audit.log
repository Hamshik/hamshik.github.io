++++++++++++++++          gpg --symmetric --cipher-algo AES256 --batch --passphrase "${GPG_PASSWORD}" backup.tar.gz
++++++++++++++++          rm -f backup.tar.gz
++++++++++++++++          echo "Backup file encrypted as: backup.tar.gz.gpg."
