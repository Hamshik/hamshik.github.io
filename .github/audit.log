++++++++++++++++++++++++++          npm audit --json > $WORKFLOW_DIR/audit-report.json || echo "Vulnerabilities found."
++++++++++++++++++++++++++          cat $WORKFLOW_DIR/audit-report.json
++++++++++++++++++++++++++        continue-on-error: true
