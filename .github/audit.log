++++++++++++++++++++++++++++++++++++++++++++++++++++          for file in dist/*; do
++++++++++++++++++++++++++++++++++++++++++++++++++++            if [[ "$file" == *.gpg ]]; then
++++++++++++++++++++++++++++++++++++++++++++++++++++              gpg --batch --passphrase "${GPG_PASSWORD}" --verify "$file" \
++++++++++++++++++++++++++++++++++++++++++++++++++++                && echo "Valid signature for $file" \
++++++++++++++++++++++++++++++++++++++++++++++++++++                || echo "Invalid signature detected in $file"
++++++++++++++++++++++++++++++++++++++++++++++++++++            fi
++++++++++++++++++++++++++++++++++++++++++++++++++++          done
++++++++++++++++++++++++++++++++++++++++++++++++++++          echo "GPG signature validation completed."
++++++++++++++++++++++++++++++++++++++++++++++++++++
++++++++++++++++++++++++++++++++++++++++++++++++++++      # Step 4: Perform Security Scan
